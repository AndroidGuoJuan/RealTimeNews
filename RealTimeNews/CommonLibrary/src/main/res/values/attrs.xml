<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义标题栏Item-->
    <declare-styleable name="TitleBar">
        <attr name="title_text" format="string" />
        <attr name="title_size" format="dimension" />
        <attr name="title_color" format="color" />
        <attr name="left_icon" format="reference" />
        <attr name="left_text" format="string" />
        <attr name="right_icon" format="reference" />
        <attr name="right_text" format="string" />
        <attr name="has_back" format="boolean" />
    </declare-styleable>

    <!--底部导航Item-->
    <declare-styleable name="NavigateItemView">
        <attr name="navigation_item_icon_normal" format="reference" />
        <attr name="navigation_item_icon_selected" format="reference" />
        <attr name="navigation_item_name" format="string" />
        <attr name="navigation_item_textsize" format="dimension" />
        <attr name="navigation_item_color" format="color" />
    </declare-styleable>
    <!--viewpager指示器-->
    <attr name="item_count" format="integer"></attr>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="item_count" />
    </declare-styleable>

    <!--轮播ViewFlow-->
    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer" />
    </declare-styleable>
    <!--轮播圆点指示器-->
    <declare-styleable name="CircleFlowIndicator">
        <attr name="activeColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="centered" format="boolean" />
        <attr name="fadeOut" format="integer" />
        <attr name="inactiveType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="circleSeparation" format="dimension" />
        <attr name="activeRadius" format="dimension" />
    </declare-styleable>
    <!--轮播标题指示器-->
    <declare-styleable name="TitleFlowIndicator">
        <attr name="titlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="clipPadding" format="dimension" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedBold" format="boolean" />
        <attr name="selectedSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="footerLineHeight" format="dimension" />
        <attr name="footerColor" format="color" />
        <attr name="footerTriangleHeight" format="dimension" />
        <attr name="customTypeface" format="string" />
    </declare-styleable>
    <!--下拉刷新JellyRefreshLayout-->
    <declare-styleable name="JellyRefreshLayout">
        <attr name="jellyColor" format="color" />
        <attr name="headerHeight" format="dimension" />
        <attr name="pullHeight" format="dimension" />
        <attr name="triggerHeight" format="dimension" />
    </declare-styleable>
    <!--仿IOS滚动滑轮控件-->
    <declare-styleable name="androidWheelView">
        <attr name="awv_textsize" format="integer" />
        <attr name="awv_lineSpace" format="float" />
        <attr name="awv_centerTextColor" format="integer" />
        <attr name="awv_outerTextColor" format="integer" />
        <attr name="awv_dividerTextColor" format="integer" />
        <attr name="awv_itemsVisibleCount" format="integer" />
        <attr name="awv_isLoop" format="boolean" />
        <attr name="awv_initialPosition" format="integer" />
        <attr name="awv_scaleX" format="float" />
    </declare-styleable>
</resources>